{
  "name": "starterkit",
  "version": "1.0.0",
  "description": "Project starter kit",
  "author": "Sheshbabu Chinnakonda",
  "license": "MIT",
  "main": "./dist/index.js",
  "scripts": {
    "start": "node ./dist",
    "dev": "npm run dev:server & npm run dev:client",
    "build": "rm -rf ./dist && npm run build:server && npm run build:client",
    "dev:server": "nodemon -q -e ts -w ./src/server -x ts-node ./src/server/index.ts",
    "dev:client": "webpack-dev-server --mode=development --config ./src/client/webpack.config.js",
    "build:server": "tsc --build ./src/server/tsconfig.server.json",
    "build:client": "webpack --mode=production --progress --profile --config ./src/client/webpack.config.js",
    "migrate": "node-pg-migrate -m ./src/server/migrations -j sql up",
    "migrate:dry": "node-pg-migrate -m ./src/server/migrations -j sql up",
    "migrate:create": "node-pg-migrate -m ./src/server/migrations -j sql create",
    "lint": "eslint --ext .ts,.tsx .",
    "precommit": "lint-staged",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{css,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "chrome-launcher": "0.10.7",
    "compression": "1.7.4",
    "date-fns": "1.30.1",
    "express": "4.17.0",
    "frappe-charts": "1.5.5",
    "lighthouse": "6.2.0",
    "node-pg-migrate": "3.20.0",
    "pg": "7.11.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "semantic-ui-calendar-react": "0.15.0",
    "semantic-ui-css": "2.4.1",
    "semantic-ui-react": "0.86.0",
    "snyk": "^1.374.0"
  },
  "devDependencies": {
    "@types/compression": "0.0.36",
    "@types/date-fns": "2.6.0",
    "@types/express": "4.16.1",
    "@types/node": "12.0.2",
    "@types/react": "16.8.18",
    "@types/react-dom": "16.8.4",
    "@typescript-eslint/eslint-plugin": "1.9.0",
    "@typescript-eslint/parser": "1.9.0",
    "awesome-typescript-loader": "5.2.1",
    "css-loader": "2.1.1",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.13.0",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "husky": "2.3.0",
    "lint-staged": "8.1.7",
    "mini-css-extract-plugin": "0.6.0",
    "nodemon": "1.19.0",
    "prettier": "1.17.1",
    "source-map-loader": "0.2.4",
    "ts-node": "8.1.0",
    "typescript": "3.4.5",
    "url-loader": "1.1.2",
    "webpack": "4.32.2",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.4.1"
  },
  "snyk": true
}
